#include <iostream>
using namespace std;
struct row
{
	const static int SIZE = 4;
	int size[SIZE];
};
void print(row* data,int x )
{
for (int i = 0; i < x; i++)
{
	for (int j = 0; j < row::SIZE; j++)
	{
		cout << data[i].size[j] << ' ';

	}
	cout << endl;
}
}
int main()
{
	const int x = 3;
	const int y = 4;
	row data[x] = {{ 2, 4, 5 }, { 8,7,4 }, {}};
	cout << row::SIZE << endl;
	print(data, x);
}
